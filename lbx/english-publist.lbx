\def\bpl@version{2.11}
\def\bpl@rdate{2024/07/02}

\ProvidesFile{english-publist.lbx}[\bpl@rdate\space v.\bpl@version\space
                                   biblatex-publist English localisation (JSP)]

% Inherit general localization
\InheritBibliographyExtras{english}

% Localizations
\DeclareBibliographyStrings{%
    inherit             = {english},
    parttranslationof   = {{partial translation of}{part\adddot trans\adddotspace of}},
    with                = {{with}{with}},
    impactfactor        = {{impact factor}{JIF}},
    openaccess:true     = {{OpenAccess}{OA}},
    openaccess:gold     = {{OpenAccess (gold)}{OA (gold)}},
    openaccess:green    = {{OpenAccess (green)}{OA (green)}},
    peerreview:sb       = {{single blind peer review}{single blind peer review}},
    peerreview:db       = {{double blind peer review}{double blind peer review}},
    peerreview:op       = {{open peer review}{open peer review}},
    peerreview:co       = {{collaborative peer review}{collab\adddotspace peer review}},
    peerreview:cc       = {{cascading peer review}{casc\adddotspace peer review}},
    peerreview:tp       = {{third-party peer review}{third-party peer review}},
    peerreview:pp       = {{post-publication peer review}{post-pub\adddotspace peer review}},
    peerreview:no       = {{no peer review}{no peer review}}
}

\endinput

